pipeline{
    agent any
    stages('different statge '){
        
        stage('Testing'){
            steps{
                echo 'Testing code'
            }
            
        }
        
        stage('Deploy'){
            steps{
                echo 'Deploying code'
            }
        }
        stage('Say Congrates'){
            steps{
                echo 'Congratulation for this successful deployment...wow'
            }
            
        }
    }
}
