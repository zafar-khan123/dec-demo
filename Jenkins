pipeline{
    agent any
    stages("different statge "){
        
        stage("git"){
            steps{
        sh "git branch: 'main', url: 'https://github.com/zafar-khan123/dec-demo.git"
            }
        
        }

        stage("Build"){
            steps{
                echo "Building code using jenkins"
            }
        }
        
        stage("Testing"){
            steps{
                echo "Testing code"
            }
            
        }
        
        stage("Deploy"){
            steps{
                echo "Deploying code"
            }
        }
    }
}
